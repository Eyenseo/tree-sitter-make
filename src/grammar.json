{
  "name": "make",
  "rules": {
    "makefile": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_thing"
      }
    },
    "_thing": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_assignments"
        }
      ]
    },
    "_assignments": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_assignment"
        }
      ]
    },
    "variable_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "modifier",
          "content": {
            "type": "SYMBOL",
            "name": "_var_modifier"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "var_name"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "operator",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "="
              },
              {
                "type": "STRING",
                "value": ":="
              },
              {
                "type": "STRING",
                "value": "::="
              },
              {
                "type": "STRING",
                "value": "?="
              },
              {
                "type": "STRING",
                "value": "+="
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "_until_newline"
                },
                "named": true,
                "value": "text"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_NL"
        }
      ]
    },
    "_var_modifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "export"
          },
          "named": true,
          "value": "export"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "unexport"
          },
          "named": true,
          "value": "unexport"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "override"
          },
          "named": true,
          "value": "override"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "private"
          },
          "named": true,
          "value": "private"
        }
      ]
    },
    "paths": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_primary"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "IMMEDIATE_TOKEN",
                    "content": {
                      "type": "STRING",
                      "value": ":"
                    }
                  },
                  {
                    "type": "IMMEDIATE_TOKEN",
                    "content": {
                      "type": "STRING",
                      "value": ";"
                    }
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "_primary"
              }
            ]
          }
        }
      ]
    },
    "_primary": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        }
      ]
    },
    "name": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[^\\s=\\\\]*[^\\s:=\\\\]"
            }
          }
        ]
      }
    },
    "var_name": {
      "type": "REPEAT1",
      "content": {
        "type": "ALIAS",
        "content": {
          "type": "SYMBOL",
          "name": "_var_name"
        },
        "named": true,
        "value": "_var_name"
      }
    },
    "_until_newline": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "REPEAT1",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[^\\n,\\r,\\\\,#]"
                  },
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "\\"
                      },
                      {
                        "type": "PATTERN",
                        "value": "[^\\n,\\r,\\\\,]"
                      }
                    ]
                  }
                ]
              }
            }
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[^\\n]"
            }
          }
        ]
      }
    },
    "_WS": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[ \\t\\f]"
        }
      }
    },
    "_NL": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "(\\n|\\r\\n)"
        }
      }
    },
    "_SPLIT": {
      "type": "ALIAS",
      "content": {
        "type": "IMMEDIATE_TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "\\\\(\\n|\\r\\n)"
        }
      },
      "named": false,
      "value": "\\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\s]"
    },
    {
      "type": "ALIAS",
      "content": {
        "type": "PATTERN",
        "value": "\\\\(\\n|\\r\\n)"
      },
      "named": false,
      "value": "\\\n"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_var_name"
    }
  ],
  "inline": [],
  "supertypes": []
}

